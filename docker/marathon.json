{
  "id": "/example-scheduler",
  "instances": 1,
  "cpus": 2,
  "mem": 128,
  "env": {
    "NUM_TASKS": "10000",
    "MESOS_CONNECT_TIMEOUT": "10s",
    "TASK_CPU": "0.1",
    "TASK_MEMORY": "0.1",
    "EXEC_CPU": "1",
    "EXEC_MEMORY": "128"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jdef/example-scheduler",
      "network": "HOST",
      "forcePullImage": true
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 0
  }
}
